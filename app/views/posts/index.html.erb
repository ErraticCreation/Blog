  <aside class="col-sm-4 col-md-3 col-lg-2">
    <%= render 'posts/navbar' %>
  </aside>
  <article class="col-xs-12 col-sm-8 col-md-9 col-lg-10 text-center">
    <h1>Blog</h1>
    <%= render 'posts/blog_post' %>
    <footer class="col-xs-12 col-sm-12 col-md-12 col-lg-12 text-center">
      <%= link_to 'New Post', new_post_path %>
    </footer>
  </article>

</div>

